<?xml version="1.0" encoding="UTF-8"?>
<suppressions xmlns="https://jeremylong.github.io/DependencyCheck/dependency-suppression.1.3.xsd">
    <!-- 抑制已知的安全漏洞警告 -->
    <suppress>
        <notes><![CDATA[
        Suppressing known false positives for test dependencies
        ]]></notes>
        <filePath regex="true">.*test.*</filePath>
        <cve>CVE-2021-44228</cve>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        Suppressing vulnerabilities in development-only dependencies
        ]]></notes>
        <filePath regex="true">.*h2.*</filePath>
        <cve>CVE-2022-45868</cve>
    </suppress>
    
    <suppress>
        <notes><![CDATA[
        Suppressing vulnerabilities in embedded databases used for testing
        ]]></notes>
        <filePath regex="true">.*h2.*\.jar</filePath>
        <cve>CVE-2023-25638</cve>
    </suppress>
    
    <!-- 可以根据实际情况添加更多的抑制规则 -->
</suppressions>